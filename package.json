{
  "name": "renderOSArms",
  "version": "2.0.0",
  "description": "Interactive 3D model repository for open-source firearm designs with AI-powered assistance",
  "main": "docs/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "list": "node docs/list.js",
    "pc-msg": "echo 'Updating list...' && exit 0",
    "dev": "node server/index.js",
    "start": "NODE_ENV=production node server/index.js",
    "test": "node scripts/test-ai-enhancements.js",
    "test:ai": "node scripts/test-ai-enhancements.js",
    "lint": "eslint docs/**/*.js server/**/*.js",
    "validate": "node scripts/validate-models.js"
  },
  "pre-commit": [ "pc-msg", "list" ],
  "repository": {
    "type": "git",
    "url": "git@github.com:4-b100m/renderOSArms.git"
  },
  "keywords": [
    "3d-printing",
    "firearms",
    "open-source",
    "models",
    "STL",
    "CAD",
    "three.js",
    "AI-assistant"
  ],
  "author": "RenderOSArms Community",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/4-b100m/renderOSArms/issues"
  },
  "homepage": "https://github.com/4-b100m/renderOSArms#readme",
  "dependencies": {
    "express": "^4.18.2",
    "three": "^0.158.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "pre-commit": "^1.2.2",
    "eslint": "^8.54.0",
    "nodemon": "^3.0.2"
  }
}
